{
  "name": "miranda-dashboard-backend",
  "version": "1.0.0",
  "description": "Back-end API project with node.js + express for miranda dashboard",
  "main": "app.ts",
  "scripts": {
    "start": "ts-node src/server.ts",
    "dev": "nodemon --watch \"src/**/*.ts\" --exec \"ts-node\" src/server.ts",
    "test": "jest",
    "build": "rimraf dist && tsc",
    "seed": "ts-node ./src/seedDatabase.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Davidrami12/miranda-dashboard-backend.git"
  },
  "jest": {
    "preset": "ts-jest",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Davidrami12/miranda-dashboard-backend/issues"
  },
  "homepage": "https://github.com/Davidrami12/miranda-dashboard-backend#readme",
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "moment": "^2.29.4",
    "mongoose": "^7.4.1",
    "react-uuid": "^2.0.0",
    "serverless-http": "^3.2.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "faker": "^6.6.6",
    "jest": "^29.6.1",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
